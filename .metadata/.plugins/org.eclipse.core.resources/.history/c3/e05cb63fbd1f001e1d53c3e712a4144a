public class Employee {
    private int empID;
    private String empName;

    // Constructor
    public Employee(int empID, String empName) {
        this.empID = empID;
        this.empName = empName;
    }

    // Getters and setters
    public int getEmpID() {
        return empID;
    }

    public void setEmpID(int empID) {
        this.empID = empID;
    }

    public String getEmpName() {
        return empName;
    }

    public void setEmpName(String empName) {
        this.empName = empName;
    }
}
